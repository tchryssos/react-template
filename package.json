    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.6",
    "clsx": "^1.1.1",
    "css-loader": "^3.6.0",
    "file-loader": "^5.1.0",
    "jss-plugin-extend": "^10.3.0",
